{
  "version": 2,
  "projects": {
    "nx-rsync-deployer": {
      "root": "packages/nx-rsync-deployer",
      "sourceRoot": "packages/nx-rsync-deployer/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/nx-rsync-deployer/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/nx-rsync-deployer"],
          "options": {
            "jestConfig": "packages/nx-rsync-deployer/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/nx-rsync-deployer",
            "tsConfig": "packages/nx-rsync-deployer/tsconfig.lib.json",
            "packageJson": "packages/nx-rsync-deployer/package.json",
            "main": "packages/nx-rsync-deployer/src/index.ts",
            "assets": ["packages/nx-rsync-deployer/*.md", "packages/nx-rsync-deployer/builders.json"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
